{
  "name": "constellation",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run metro:cleanport && npx react-native start --host 0.0.0.0 --port 8081",
    "android": "npm run android:wifi && (npm run android:metro >/tmp/constellation-metro.log 2>&1 &) && npm run android:install && npm run android:launch && npm run android:logs",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild --clean",
    "dev-client": "npm run metro:cleanport && npx react-native start --host 0.0.0.0 --port 8081",
    "android:install": "cd android && ./gradlew installDebug",
    "android:wifi": "adb connect ${ANDROID_DEVICE:-192.168.0.158}:5555",
    "android:launch": "adb reverse tcp:8081 tcp:8081 && adb shell am start -n com.constellation.app/.MainActivity",
    "android:metro": "npm run metro:cleanport && npx react-native start --host 0.0.0.0 --port 8081",
    "metro:cleanport": "sh -c 'pids=$(lsof -ti tcp:8081); if [ -n \"$pids\" ]; then kill -9 $pids; fi'",
    "android:logs": "sh -c 'tail -n 80 -f /tmp/constellation-metro.log & npx react-native log-android'"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-community/netinfo": "^9.3.10",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@supabase/supabase-js": "^2.49.1",
    "base64-arraybuffer": "^1.0.2",
    "dotenv": "^16.4.7",
    "expo": "~49.0.15",
    "expo-auth-session": "~5.0.2",
    "expo-build-properties": "~0.8.3",
    "expo-image-picker": "~14.3.2",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "nativewind": "^4.2.1",
    "onesignal-expo-plugin": "^2.0.0",
    "react": "18.2.0",
    "react-native": "0.72.10",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-onesignal": "^5.2.14",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-uuid": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native/metro-config": "^0.72.12",
    "@types/react": "~18.2.14",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.1.3"
  },
  "private": true
}